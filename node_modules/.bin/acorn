#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/acorn@6.4.2/node_modules/acorn/bin/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/acorn@6.4.2/node_modules/acorn/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/acorn@6.4.2/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules:/Users/berat/IdeaProjects/node_modules:/Users/berat/node_modules:/Users/node_modules:/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/acorn/bin/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/acorn/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../acorn/bin/acorn" "$@"
fi
