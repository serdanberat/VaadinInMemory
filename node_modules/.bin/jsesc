#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/jsesc@1.3.0/node_modules/jsesc/bin/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/jsesc@1.3.0/node_modules/jsesc/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/jsesc@1.3.0/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules:/Users/berat/IdeaProjects/node_modules:/Users/berat/node_modules:/Users/node_modules:/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/jsesc/bin/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/jsesc/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jsesc/bin/jsesc" "$@"
else
  exec node  "$basedir/../jsesc/bin/jsesc" "$@"
fi
