#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/ansi-html@0.0.7/node_modules/ansi-html/bin/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/ansi-html@0.0.7/node_modules/ansi-html/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/ansi-html@0.0.7/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules:/Users/berat/IdeaProjects/node_modules:/Users/berat/node_modules:/Users/node_modules:/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/ansi-html/bin/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/ansi-html/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ansi-html/bin/ansi-html" "$@"
else
  exec node  "$basedir/../ansi-html/bin/ansi-html" "$@"
fi
