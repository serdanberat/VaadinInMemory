#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/rimraf@2.7.1/node_modules/rimraf/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/rimraf@2.7.1/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules:/Users/berat/IdeaProjects/node_modules:/Users/berat/node_modules:/Users/node_modules:/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/copy-concurrently@1.0.5/node_modules/copy-concurrently/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/copy-concurrently@1.0.5/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rimraf@2.7.1/node_modules/rimraf/bin.js" "$@"
else
  exec node  "$basedir/../../../../../rimraf@2.7.1/node_modules/rimraf/bin.js" "$@"
fi
