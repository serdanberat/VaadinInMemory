#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/browserslist@4.16.6/node_modules/browserslist/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/browserslist@4.16.6/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules:/Users/berat/IdeaProjects/node_modules:/Users/berat/node_modules:/Users/node_modules:/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/core-js-compat@3.15.2/node_modules/core-js-compat/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/core-js-compat@3.15.2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../browserslist@4.16.6/node_modules/browserslist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../browserslist@4.16.6/node_modules/browserslist/cli.js" "$@"
fi
