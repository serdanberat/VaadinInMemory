#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/typescript@4.2.3/node_modules/typescript/bin/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/typescript@4.2.3/node_modules/typescript/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/typescript@4.2.3/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules:/Users/berat/IdeaProjects/node_modules:/Users/berat/node_modules:/Users/node_modules:/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/ts-loader@8.0.12_typescript@4.2.3+webpack@4.46.0/node_modules/ts-loader/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/ts-loader@8.0.12_typescript@4.2.3+webpack@4.46.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.2.3/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.2.3/node_modules/typescript/bin/tsserver" "$@"
fi
