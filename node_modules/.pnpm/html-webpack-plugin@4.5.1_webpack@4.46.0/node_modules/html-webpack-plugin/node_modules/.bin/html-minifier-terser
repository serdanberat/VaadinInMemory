#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules/html-minifier-terser/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules:/Users/berat/IdeaProjects/node_modules:/Users/berat/node_modules:/Users/node_modules:/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/html-webpack-plugin@4.5.1_webpack@4.46.0/node_modules/html-webpack-plugin/node_modules:/Users/berat/IdeaProjects/FirstProject/node_modules/.pnpm/html-webpack-plugin@4.5.1_webpack@4.46.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../html-minifier-terser@5.1.1/node_modules/html-minifier-terser/cli.js" "$@"
else
  exec node  "$basedir/../../../../../html-minifier-terser@5.1.1/node_modules/html-minifier-terser/cli.js" "$@"
fi
